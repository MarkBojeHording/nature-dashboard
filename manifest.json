{
  "manifest_version": 3,
  "name": "Nature Dashboard",
  "version": "1.0",
  "description": "A personal dashboard Chrome extension",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
